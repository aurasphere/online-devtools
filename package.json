{
  "name": "online-devtools",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "homepage": "/online-devtools",
  "dependencies": {
    "bootstrap-switch-button-react": "^1.2.0",
    "crypto-js": "^4.1.1",
    "node-jose": "^2.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.2.0",
    "react-json-pretty": "^2.2.0",
    "react-pro-sidebar": "^0.6.0",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.13.16",
    "@babel/preset-env": "^7.13.15",
    "@babel/preset-react": "^7.13.13",
    "babelify": "^10.0.0",
    "gh-pages": "^3.1.0",
    "react-scripts": "4.0.3",
    "serve": "^11.3.2"
  },
  "scripts": {
    "start": "yarn build && serve -s build",
    "live": "react-scripts start",
    "build": "react-scripts build && browserify -t [ babelify --presets [  @babel/preset-env @babel/preset-react ] ] src/index.js -o build/app.js",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
